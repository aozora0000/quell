{
    "name": "aozora0000/laravel-querial",
    "description": "laravel request to query builder for search",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "aozora0000",
            "email": "aozora0000@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Querial\\": "src/Querial"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/http": "^9.39.0",
        "illuminate/database": "^9.39.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.78",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.18",
        "ext-pdo": "*"
    },
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan analyze . --level=9",
        "fixer": "php-cs-fixer fix",
        "cs": "phpcs --standard=phpcs.xml --extensions=php ."
    }
}
